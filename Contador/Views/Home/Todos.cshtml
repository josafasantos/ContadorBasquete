@using ContadorBasquete.Models;
@model List<LancarPontos>



<h1>Todos Resultados</h1>
<table class="table">
    <thead class="thead-default">
        
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Data</th>
            <th scope="col">Pontos</th>
        </tr>
    </thead>
    <tbody>



        @{ foreach (var ponto in Model)
           {
                <tr>
                    <td>@ponto.Id</td>
                    <td>@ponto.Date</td>
                    <td>@ponto.Pontos</td>
                </tr>
           }
         }


                

     </tbody>
</table>
<a class="btn btn-primary" asp-area="" asp-controller="Home" asp-action="Relatorio">Voltar</a>
